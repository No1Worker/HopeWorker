<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.no1worker.user">

    <application
        android:name=".UserApplication"
        android:allowBackup="true"
        android:icon="@mipmap/user_ic_launcher"
        android:label="@string/user_app_name"
        android:roundIcon="@mipmap/user_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".UserLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserRegisterActivity" />
    </application>

</manifest>